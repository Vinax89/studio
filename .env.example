# API key for the Firebase project
NEXT_PUBLIC_FIREBASE_API_KEY=REPLACE_WITH_VALUE

# Auth domain for the Firebase project
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=REPLACE_WITH_VALUE

# Firebase project identifier
NEXT_PUBLIC_FIREBASE_PROJECT_ID=REPLACE_WITH_VALUE

# Default Cloud Storage bucket
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=REPLACE_WITH_VALUE

# Firebase Cloud Messaging sender ID
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=REPLACE_WITH_VALUE

# Firebase app ID
NEXT_PUBLIC_FIREBASE_APP_ID=REPLACE_WITH_VALUE

# OpenAI API key from https://platform.openai.com/
OPENAI_API_KEY=REPLACE_WITH_VALUE

# Days to retain files before deletion (default: 30)
RETENTION_DAYS=30
